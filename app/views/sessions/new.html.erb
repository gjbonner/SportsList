
<h1>LOGIN</h1>
<center>
  <div class="jumbotron">


  <h3><%= flash[:notice] %></h3>

<%= form_tag sessions_path do %>
  <%= label_tag "Username" %>
  <%= text_field_tag :name %><br>
  <%= label_tag "Password" %>
  <%= password_field_tag :password %><br>
  <%= submit_tag "Sign In" %>
<% end %>
</div>
</center>
